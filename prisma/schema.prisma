generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model jamur {
   id           Int     @id @default(autoincrement())
   nama         String  
   deskripsi    String  
   jenis        String
   media_tanam  String  
}

model gambar {
   id       Int     @id @default(autoincrement())
   gambar1  String  
   gambar2  String  
   gambar3  String  
}
